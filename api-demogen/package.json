{
  "name": "@solplay/api-demogen",
  "version": "1.0.0",
  "description": "Professional demo generation API service using v0 SDK with comprehensive security and logging",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:debug": "nodemon --watch src --ext ts --exec \"node --inspect=9229 -r ts-node/register src/index.ts\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "security:audit": "npm audit",
    "security:scan": "snyk test",
    "docker:build": "docker build -t api-demogen:latest .",
    "docker:run": "docker run -p 3001:3001 api-demogen:latest"
  },
  "keywords": [
    "demo-generation",
    "v0-sdk",
    "api",
    "synthetic-data",
    "react-components",
    "professional-ui"
  ],
  "author": "SolPlay DemoGen Team",
  "license": "MIT",
  "dependencies": {
    "@azure/identity": "^3.2.4",
    "@azure/keyvault-secrets": "^4.7.0",
    "@azure/openai": "^2.0.0-beta.1",
    "@azure/storage-blob": "^12.15.0",
    "axios": "^1.4.0",
    "azure-storage": "^2.10.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dompurify": "^3.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-slow-down": "^1.6.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "openai": "^4.63.0",
    "pg": "^8.11.1",
    "redis": "^4.6.7",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0",
    "v0-sdk": "^0.12.0",
    "winston": "^3.10.0",
    "yamljs": "^0.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/dompurify": "^3.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/joi": "^17.2.3",
    "@types/jsdom": "^21.1.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.2",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.2",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "snyk": "^1.1193.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rajesh-ms/hackathon-solplay-demogen.git"
  }
}
